	<counter def="counter-1"/>
	<!-- 1 -->
        <counter-def id="counter-1" name="1">
            <filter-date field="regDt" cmp="ge" day="1" month="0"/>  - INQRY_QUEUE_INFO.dt_reg
            <filter-date field="regDt" cmp="le"/>
            <age-range def="range-1"/>
        </counter-def>
		
                <counter def="counter-1.1"/>
	<!-- 1.1 -->
        <counter-def id="counter-1.1" name="1.1">
            <filter-date field="regDt" cmp="ge" day="1" month="0"/>
            <filter-date field="regDt" cmp="le"/>
            <age-range def="range-16"/>
        </counter-def>
		
                <counter def="counter-2"/>
	<!-- 2 -->
        <counter-def id="counter-2" name="2">
            <filter-date field="regDt" cmp="ge" day="1" month="0"/>
            <filter-date field="regDt" cmp="le"/>
            <filter field="fromPortal" cmp="eq" value="true"/>  
			select isi.id_inqry as id_inqry from inqry_sys_interact isi where isi.id_sys_interact in (1825, 1916)
			- где 1825 - SMEV, 1916 - CONCENTRATOR (CDict87SysInteraction) - возвращается List id_inqry			
            <age-range def="range-16"/>
        </counter-def>
		
                <counter def="counter-3"/>
	<!-- 3 -->
        <counter-def id="counter-3" name="3">
            <filter-date field="regDt" cmp="ge" day="1" month="0"/>
            <filter-date field="regDt" cmp="le"/>
            <filter field="fromPortal" cmp="eq" value="false"/>
            <age-range def="range-16"/>
        </counter-def>
		
                <counter def="counter-4"/>
	<!-- 4 -->
        <counter-def id="counter-4" name="4">
            <filter-date field="regDt" cmp="ge" educDate="true"/>
            <filter-date field="regDt" cmp="le"/>
            <filter-date field="inUchDt" cmp="ge" educDate="true"/>           - INQRYCHLDINUCH.inuch_dt
            <filter-date field="inUchDt" cmp="lt" educDate="true" yearOffset="1"/>
            <age-range def="range-16"/>
        </counter-def>
        <!-- 4 учесть детей, которым в текущем учебном году была изменена дата зачисления на текущий учебный год -->
        <counter-def id="counter-4.prev_inuch_dt" name="4">
            <filter-date field="regDt" cmp="lt" educDate="true"/>
            <filter-date field="prevInUchDt" cmp="ge" educDate="true" yearOffset="1"/>   - AUD_INQRYCHLDINUCH.inuch_dt
			Отбор по условию ???
            <filter-date field="inUchDt" cmp="ge" educDate="true"/>
            <filter-date field="inUchDt" cmp="lt" educDate="true" yearOffset="1"/>
            <age-range def="range-16"/>
        </counter-def>
		
